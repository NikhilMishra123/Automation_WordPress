<p>Sign In</p>
<%= form_with scope: :user, :url => user_login_path, local: true do |form| %>
  <p>
    <%= form.label 'email:' %><br />
    <%= form.text_field :email %>
  </p>
  <p>
    <%= form.label 'password:' %><br />
    <%= form.password_field :password %>
  </p>
  <p>
    <%= form.submit 'Sign In' %>
  </p>
<% end %>
